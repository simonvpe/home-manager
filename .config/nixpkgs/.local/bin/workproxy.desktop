#!/usr/bin/env bash

readonly process_name=workproxyprocess
readonly workproxy='ssh -f workproxy sleep infinity'
readonly worksocks='ssh -f worksocks sleep infinity'

if [[  $1 == start ]]; then
    exec -a ${process_name} bash -c "${workproxy} && ${worksocks}"
elif [[ $1 == stop ]]; then
    pkill -f ${process_name}
else
    >&2 echo "Usage: $0 start|stop"
fi
